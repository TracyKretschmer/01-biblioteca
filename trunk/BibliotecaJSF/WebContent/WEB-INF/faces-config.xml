<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
    		  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    		  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    		  version="2.0">
	
	<application id="BibliotecaJSF">
		
	</application>

	<navigation-rule>
		<navigation-case>
			<from-outcome>menuInicial</from-outcome>
			<to-view-id>/public/menuInicial.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Todas</display-name>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>manterUsuario</from-outcome>
			<to-view-id>/public/manterUsuario.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
